############################
#   Object Properties
############################

# Object Property: :hasDamageModel (:hasDamageModel)

AnnotationAssertion(rdfs:isDefinedBy :hasDamageModel "This is an object property that displays the relationship between a weapon and its damage type: hit scan or projectile.")

# Object Property: :hasRarity (:hasRarity)

AnnotationAssertion(rdfs:isDefinedBy :hasRarity "This property displays the rarity of the object.")
FunctionalObjectProperty(:hasRarity)
ObjectPropertyDomain(:hasRarity :Weapons)
ObjectPropertyRange(:hasRarity :Rarity)

# Object Property: :isMadeOf (:isMadeOf)

AnnotationAssertion(rdfs:isDefinedBy :isMadeOf "This property is specific to the class building_object and displays the Object_material that it is made from.")
InverseObjectProperties(:isMadeOf :isUsedToMake)
ObjectPropertyDomain(:isMadeOf :Building_Object)
ObjectPropertyRange(:isMadeOf :Building_Material)

# Object Property: :isTypeOf (:isTypeOf)

AnnotationAssertion(rdfs:isDefinedBy :isTypeOf "this property is an extion of a 'is a' object property.")

# Object Property: :isUsedBy (:isUsedBy)

AnnotationAssertion(rdfs:isDefinedBy :isUsedBy "This property is used to display the relationship between one individual that uses the other. For example ammo is used by a weapon.")
InverseObjectProperties(:isUsedBy :uses)
ObjectPropertyDomain(:isUsedBy :Ammo)
ObjectPropertyRange(:isUsedBy :Weapons)

# Object Property: :isUsedToMake (:isUsedToMake)

AnnotationAssertion(rdfs:isDefinedBy :isUsedToMake "This object property is the inverse of 'isMadeOf' and is specific to the relationship between building_object and Building_material.")
ObjectPropertyDomain(:isUsedToMake :Building_Material)
ObjectPropertyRange(:isUsedToMake :Building_Object)

# Object Property: :placed_on (:placed_on)

AnnotationAssertion(rdfs:isDefinedBy :placed_on "This object property is specific to the class 'Traps/Equipment' and shows the building_object that is required for the trap or equipment to operate.")
ObjectPropertyDomain(:placed_on <http://www.semanticweb.org/Fortnite_Ontology_Nicholas_Thompson#Traps/Equipment>)
ObjectPropertyRange(:placed_on ObjectSomeValuesFrom(:placed_on :Building_Object))

# Object Property: :uses (:uses)

AnnotationAssertion(rdfs:isDefinedBy :uses "This is the opposite of the object property 'isUsedBy' and shows the relationship between individuals that rely on each other. For example, a weapon uses ammo.")
ObjectPropertyDomain(:uses :Weapons)
ObjectPropertyRange(:uses :Ammo)



